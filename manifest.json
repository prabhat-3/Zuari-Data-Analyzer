{
  "manifest_version": 3,
  "name": "Excel Data Analyzer",
  "version": "1.0",
  "description": "Analyze Excel files and display the results using a browser extension.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ]
}
